{
  "name": "git-notch",
  "displayName": "git-notch",
  "description": "Better git tools through the command palette",
  "version": "0.1.0",
  "preview": true,
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "dev": "webpack --mode development --watch",
    "lint": "tslint -p ./",
    "pretest": "npm run webpack",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "simple-git": "^1.128.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "git",
    "git-plus"
  ],
  "license": "SEE LICENSE IN license",
  "publisher": "timomeh",
  "activationEvents": [
    "onCommand:gitNotch.stage",
    "onCommand:gitNotch.commit"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitNotch.stage",
        "title": "Git Notch: Stage"
      },
      {
        "command": "gitNotch.commit",
        "title": "Git Notch: Commit"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timomeh/git-notch.git"
  }
}
